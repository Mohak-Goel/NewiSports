<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    android:orientation="vertical"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"

    android:layout_height="wrap_content">


    <TextView
        android:id="@+id/textView24"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="30dp"
        android:layout_marginTop="10dp"
        android:layout_marginRight="30dp"
        android:layout_marginBottom="2dp"
        android:background="@drawable/editbackgound"
        android:text="UNIVERSITY NAME-:"
        android:textColor="@android:color/black"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/universityn1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="30dp"
        android:layout_marginTop="10dp"
        android:layout_marginRight="30dp"
        android:layout_marginBottom="2dp"
        android:background="@drawable/submit_button_style"
        android:text="KIIT UNIVERSITY"
        android:textColor="@color/design_default_color_on_secondary"

        android:textSize="30dp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/textVew24"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="30dp"
        android:layout_marginTop="10dp"
        android:layout_marginRight="30dp"
        android:layout_marginBottom="2dp"
        android:background="@drawable/editbackgound"
        android:text="COACH NAME-:"
        android:textColor="@android:color/black"
        android:textStyle="bold" />
    <TextView
        android:id="@+id/universitycoachname"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="30dp"
        android:layout_marginTop="10dp"

        android:layout_marginRight="30dp"
        android:layout_marginBottom="20dp"
        android:text="Mohak Goel"
        android:background="@drawable/submit_button_style"
        android:textColor="@color/design_default_color_on_secondary"
        android:textSize="20dp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/textView26"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="==========================================="
        android:textAlignment="center"
        android:textColor="@android:color/black"
        android:textStyle="bold" />

</LinearLayout>